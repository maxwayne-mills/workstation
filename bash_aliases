alias ls='ls -l'
alias lt='ls -lairt'
alias ll='ls -laiFZ'
alias ld='ls -ld'
alias mv='mv -v'
alias cp='cp -iv'
alias cls='clear'
alias mkdir='mkdir -vp'
alias rm='rm -v'
alias netstat='netstat -tulpn'
alias vbl='VBoxManage list vms'
alias vbc='VBoxManage controlvm $1'
alias vbu='VBoxManage unregistervm $1'
